{
    "name": "vitorarantes/array-fly",
    "description": "ArrayFly is a simple PHP Library that allows you to modify an array on the fly.",
    "keywords": ["array", "php", "arrayfly", "file"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitor Arantes",
            "email": "hello@vitorarantes.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArrayFly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArrayFly\\Test\\": "tests/"
        }
    }
}
